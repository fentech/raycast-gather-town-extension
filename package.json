{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gather-town",
  "title": "Gather.town",
  "description": "Manage your Gather.town availability, camera, and mic",
  "icon": "command-icon.png",
  "author": "joe_fensler",
  "categories": [
    "Productivity",
    "Developer Tools",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-camera",
      "title": "Toggle Camera",
      "subtitle": "Gather",
      "description": "Toggle your camera in Gather",
      "mode": "no-view"
    },
    {
      "name": "quit-gather",
      "title": "Quit Gather",
      "subtitle": "Gather",
      "description": "Close the Gather app",
      "mode": "no-view"
    },
    {
      "name": "toggle-microphone",
      "title": "Toggle Microphone",
      "subtitle": "Gather",
      "description": "Toggle your microphone in Gather",
      "mode": "no-view"
    },
    {
      "name": "change-availability",
      "title": "Change Availability",
      "subtitle": "Gather",
      "description": "Change your availability status in Gather",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
